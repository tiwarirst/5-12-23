#include <stdio.h>

int main()
{
    int n,count;
    printf("enter the no:");
    scanf("%d",&n);
    if (n==0)
    printf("prime number is started from 1");
    else if(n<0)
    printf("enter valid number .prime number is non negative");
    else
    {for(int i=2;i<n;i++)
  {if(n%i==0)
    {count ++;}}  
    if(count==0)

    printf("entered number is prime");
    else
    printf("enterd number is not prime");}
    return 0;
}
